      <bpmn:outgoing>MitzuziehendePersonenVorhanden</bpmn:outgoing>
      <bpmn:outgoing>MitzuziehendePersonenNichtVorhanden</bpmn:outgoing>
    </bpmn:exclusiveGateway>
